<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Carritos</title>
</head>
<body>
    <%carts.forEach((c) => {%>
        <br>
        <div>
            <h2>Carrito <%=c.uuid%></h2>
            <div>
                <p>Productos:</p>
                <% c.products.forEach((p) => { %>
                    <span><%=p.name%>, </span>
               <% }) %>
            </div>
        </div>
   <% })%>
</body>
</html>